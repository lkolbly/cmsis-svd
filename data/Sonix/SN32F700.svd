              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Busy</name>
                  <description>Flash operation is not completed</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>End</name>
                  <description>Flash operation is completed</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>

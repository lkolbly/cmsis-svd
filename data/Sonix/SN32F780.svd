      <version>1.0</version>
      <description>UART0</description>
      <groupName>UART</groupName>
      <baseAddress>0x40016000</baseAddress>
      <size>12</size>
      <access>read-write</access>

      <addressBlock>
        <offset>0</offset>
        <size>0x2000</size>
        <usage>registers</usage>
      </addressBlock>

      <interrupt>
        <name>UART0</name>
        <value>13</value>
        <description>UART0</description>
      </interrupt>

      <registers>
        <!-- UART0_RB: UART0 Receiver Buffer Register -->
        <register>
          <name>RB</name>
          <description>Offset:0x00 UARTn Receiver Buffer Register</description>
          <addressOffset>0x00</addressOffset>
          <size>32</size>
          <access>read-only</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0x000000FF</resetMask>

          <fields>
            <field>
              <name>RB</name>
              <description>The received byte in UART RX FIFO</description>
              <bitRange>[7:0]</bitRange>
              <access>read-only</access>
            </field>
          </fields>
        </register>

        <!-- UART0_TH: UART0 Transmit Holding Register -->
        <register>
          <name>TH</name>
          <description>Offset:0x00 UARTn Transmit Holding Register</description>
          <alternateRegister>RB</alternateRegister>
          <addressOffset>0x00</addressOffset>
          <size>32</size>
          <access>write-only</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0x000000FF</resetMask>

          <fields>
            <field>
              <name>TH</name>
              <description>The byte to be transmitted in UART TX FIFO when transmitter is available</description>
              <bitRange>[7:0]</bitRange>
              <access>write-only</access>
            </field>
          </fields>
        </register>
        
        <!-- UART0_DLL: UART0 Divisor Latch LSB Register -->
        <register>
          <name>DLL</name>
          <description>Offset:0x00 UARTn Divisor Latch LSB Register</description>
          <alternateRegister>RB</alternateRegister>
          <addressOffset>0x00</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0x000000FF</resetMask>

          <fields>
            <field>
              <name>DLL</name>
              <description>DLL and DLM register determines the baud rate of UARTn</description>
              <bitRange>[7:0]</bitRange>
              <access>read-write</access>
            </field>
          </fields>
        </register>

        <!-- UART0_DLM: UART0 Divisor Latch MSB Register -->
        <register>
          <name>DLM</name>
          <description>Offset:0x04 UARTn Divisor Latch MSB Register</description>
          <addressOffset>0x04</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0x000000FF</resetMask>

          <fields>
            <field>
              <name>DLM</name>
              <description>DLL and DLM register determines the baud rate of USARTn</description>
              <bitRange>[7:0]</bitRange>
              <access>read-write</access>
            </field>
          </fields>
        </register>
        
	<!-- UART0_IE: UART0 Interrupt Enable Register -->
        <register>
          <name>IE</name>
          <description>Offset:0x04 UARTn Interrupt Enable Register</description>
          <alternateRegister>DLM</alternateRegister>
          <addressOffset>0x04</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0x00000317</resetMask>

          <fields>
            <field>
              <name>RDAIE</name>
              <description>RDA interrupt enable</description>
              <bitRange>[0:0]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <value>0</value>
                  <description>Disable RDA interrupt</description>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <value>1</value>
                  <description>Enable RDA interrupt</description>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>THREIE</name>
              <description>THRE interrupt enable</description>
              <bitRange>[1:1]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <value>0</value>
                  <description>Disable THRE interrupt</description>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <value>1</value>
                  <description>Enable THRE interrupt</description>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>RLSIE</name>
              <description>RLS interrupt enable</description>
              <bitRange>[2:2]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <value>0</value>
                  <description>Disable RLS interrupt</description>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <value>1</value>
                  <description>Enable RLS interrupt</description>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>TEMTIE</name>
              <description>TEMT interrupt enable</description>
              <bitRange>[4:4]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <value>0</value>
                  <description>Disable TEMT interrupt</description>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <value>1</value>
                  <description>Enable TEMT interrupt</description>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>ABEOIE</name>
              <description>ABE0 interrupt enable</description>
              <bitRange>[8:8]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <value>0</value>
                  <description>Disable ABEO interrupt</description>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <value>1</value>
                  <description>Enable ABEO interrupt</description>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>ABTOIE</name>
              <description>ABT0 interrupt enable</description>
              <bitRange>[9:9]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <value>0</value>
                  <description>Disable ABTO interrupt</description>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <value>1</value>
                  <description>Enable ABTO interrupt</description>
                </enumeratedValue>
              </enumeratedValues>
            </field>
          </fields>
        </register>
        
	<!-- UART0_II: UART0 Interrupt Identification Register -->
        <register>
          <name>II</name>
          <description>Offset:0x08 UARTn Interrupt Identification Register</description>
          <addressOffset>0x08</addressOffset>
          <size>32</size>
          <access>read-only</access>
          <resetValue>0x00000041</resetValue>
          <resetMask>0x000003CF</resetMask>

          <fields>
            <field>
              <name>INTSTATUS</name>
              <description>Interrupt status</description>
              <bitRange>[0:0]</bitRange>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Pending</name>
                  <value>0</value>
                  <description>As least 1 interrupt is pending</description>
                </enumeratedValue>
                <enumeratedValue>
                  <name>No interrupt</name>
                  <description>No interrupt</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>INTID</name>
              <description>Interrupt ID of RX FIFO</description>
              <bitRange>[3:1]</bitRange>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>3a</name>
                  <description>THRE interrupt</description>
                  <value>1</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>2a</name>
                  <description>RDA (Receive Data Available)</description>
                  <value>2</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>1</name>
                  <description>RLS (Receive Line Status)</description>
                  <value>3</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>3b</name>
                  <description>TEMT interrupt</description>
                  <value>7</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>FIFOEN</name>
              <description>Equal to FIFOEN bits in USARTn_FIFOCTRL register</description>
              <bitRange>[7:6]</bitRange>
              <access>read-only</access>
            </field>
            <field>
              <name>ABEOIF</name>
              <description>ABEO interrupt flag</description>
              <bitRange>[8:8]</bitRange>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Not end</name>
                  <value>0</value>
                  <description>Auto-baud has not finished</description>
                </enumeratedValue>
                <enumeratedValue>
                  <name>End</name>
                  <description>Auto-baud has finished and interrupt is enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>ABTOIF</name>
              <description>ABTO interrupt flag</description>
              <bitRange>[9:9]</bitRange>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Not Time-out</name>
                  <value>0</value>
                  <description>Auto-baud has not timed out</description>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Time-out</name>
                  <description>Auto-baud has timed out and interrupt is enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
          </fields>
        </register>

	<!-- UART0_FIFOCTRL: UART0 FIFO Control Register -->
        <register>
          <name>FIFOCTRL</name>
          <description>Offset:0x08 UARTn FIFO Control Register</description>
          <addressOffset>0x08</addressOffset>
          <size>32</size>
          <access>write-only</access>
          <resetValue>0x00000001</resetValue>
          <resetMask>0x000000C1</resetMask>

          <fields>
	    <field>
              <name>FIFOEN</name>
              <description>FIFO enable</description>
              <bitRange>[0:0]</bitRange>
              <access>write-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>No effect</name>
                  <description>No effect</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>Enable FIFO</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>RXTL</name>
              <description>RX trigger level</description>
              <bitRange>[7:6]</bitRange>
              <access>write-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Trigger level 0</name>
                  <description>1 character</description>
                  <value>0</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
          </fields>
        </register>
        
	<!-- UART0_LC: UART0 Line Control Register -->
        <register>
          <name>LC</name>
          <description>Offset:0x0C UARTn Line Control Register</description>
          <addressOffset>0x0C</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0x000000FF</resetMask>

          <fields>
	    <field>
              <name>WLS</name>
              <description>Word length selection</description>
              <bitRange>[1:0]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>5-bit</name>
                  <description>5-bit character</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>6-bit</name>
                  <description>6-bit character</description>
                  <value>1</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>7-bit</name>
                  <description>7-bit character</description>
                  <value>2</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>8-bit</name>
                  <description>8-bit character</description>
                  <value>3</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>SBS</name>
              <description>Stop bit selection</description>
              <bitRange>[2:2]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>1 stop bit</name>
                  <description>1 stop bit</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>2 stop bit</name>
                  <description>2 stop bit (1.5 stop bit if WLS=0)</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>PE</name>
              <description>Parity enable</description>
              <bitRange>[3:3]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>Disable parity generation and checking</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>Enable parity generation and checking</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>PS</name>
              <description>Parity selection</description>
              <bitRange>[5:4]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>0</name>
                  <description>Odd parity</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>1</name>
                  <description>Even parity</description>
                  <value>1</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>2</name>
                  <description>Forced 1 sticky parity</description>
                  <value>2</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>3</name>
                  <description>Forced 0 sticky parity</description>
                  <value>3</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
	    <field>
              <name>BC</name>
              <description>Break control</description>
              <bitRange>[6:6]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>Disable break transmission</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>Enable break transmission</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>DLAB</name>
              <description>Divisor Latch access</description>
              <bitRange>[7:7]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>Disable access to Divisor Latch</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>Enable access to Divisor Latch</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>            
          </fields>
        </register>

        <!-- UART0_LS: UART0 Line Status Register -->
        <register>
          <name>LS</name>
          <description>Offset:0x14 UARTn Line Status Register</description>
          <addressOffset>0x14</addressOffset>
          <size>32</size>
          <access>read-only</access>
          <resetValue>0x00000060</resetValue>
          <resetMask>0x000000FF</resetMask>

          <fields>
	    <field>
              <name>RDR</name>
              <description>Receiver data ready flag</description>
              <bitRange>[0:0]</bitRange>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Not ready</name>
                  <description>UARTn_RB FIFO is empty</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Ready</name>
                  <description>UARTn_RB FIFO contains valid data</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>OE</name>
              <description>Overrun error flag</description>
              <bitRange>[1:1]</bitRange>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>No</name>
                  <description>No overrun error</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Overrun error</name>
                  <description>Overrun error status is active</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
	    <field>
              <name>PE</name>
              <description>Parity error flag</description>
              <bitRange>[2:2]</bitRange>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>No</name>
                  <description>No parity error</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Parity error</name>
                  <description>Parity error status is active</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>FE</name>
              <description>Framing error flag</description>
              <bitRange>[3:3]</bitRange>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>No</name>
                  <description>No framing error</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Framing error</name>
                  <description>Framing error status is active</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>BI</name>
              <description>Break interrupt flag</description>
              <bitRange>[4:4]</bitRange>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>No</name>
                  <description>No break interrupt</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Break interrupt</name>
                  <description>Break interrupt status is active</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>THRE</name>
              <description>THR empty flag</description>
              <bitRange>[5:5]</bitRange>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Not empty</name>
                  <description>THR contains valid data</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Empty</name>
                  <description>THR (TX FIFO) is empty</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>TEMT</name>
              <description>Transmitter empty flag</description>
              <bitRange>[6:6]</bitRange>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Not empty</name>
                  <description>THR and/or TSR contains valid data</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Empty</name>
                  <description>THR and TSR are both empty</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>RXFE</name>
              <description>Receiver FIFO error flag</description>
              <bitRange>[7:7]</bitRange>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>No RX FIFO error</name>
                  <description>UARTn_RB contains no UART RX errors</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>RX FIFO error</name>
                  <description>UARTn_RB contains at least 1 UART RX error</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
          </fields>
        </register>

        <!-- UART0_SP: UART0 Scratch Pad Register -->
        <register>
          <name>SP</name>
          <description>Offset:0x1C UARTn Scratch Pad Register</description>
          <addressOffset>0x1C</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0x000000FF</resetMask>

          <fields>
	    <field>
              <name>PAD</name>
              <description>Pad informaton</description>
              <bitRange>[7:0]</bitRange>
              <access>read-write</access>
            </field>
          </fields>
        </register>
        
        <!-- UART0_ABCTRL: UART0 Auto-baud Control Register -->
        <register>
          <name>ABCTRL</name>
          <description>Offset:0x20 UARTn Auto-baud Control Register</description>
          <addressOffset>0x20</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0x00000307</resetMask>

          <fields>
	    <field>
              <name>START</name>
              <description>Auto-baud run bit</description>
              <bitRange>[0:0]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Stop</name>
                  <description>Auto-baud is not running</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Start</name>
                  <description>Auto-baud ids running</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>MODE</name>
              <description>Auto-baud mode selection</description>
              <bitRange>[1:1]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Mode 0</name>
                  <description>Auto-baud mode 0</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Mode 1</name>
                  <description>Auto-baud mode 1</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>AUTORESTART</name>
              <description>Restart mode selection</description>
              <bitRange>[2:2]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>No restart</name>
                  <description>No restart</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Restart</name>
                  <description>Auto restart in case of timeout</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>ABEOIFC</name>
              <description>Clear ABEOIF flag</description>
              <bitRange>[8:8]</bitRange>
              <access>write-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>No effect</name>
                  <description>No effect</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Clear</name>
                  <description>Clear ABEOIF bit</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>ABTOIFC</name>
              <description>Clear ABTOIF flag</description>
              <bitRange>[9:9]</bitRange>
              <access>write-only</access>
              <enumeratedValues>

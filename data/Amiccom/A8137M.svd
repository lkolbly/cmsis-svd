                </enumeratedValue>
                <enumeratedValue>
                  <name>1</name>
                  <description>I2C_SCL connect to P0_10 and I2C_SCL connect to P0_11.</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>

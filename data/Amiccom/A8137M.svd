              <enumeratedValues>
                <enumeratedValue>
                  <name>0</name>
                  <description>I2C Master controller not busy</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>1</name>
                  <description>I2C Master controller is busy</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>

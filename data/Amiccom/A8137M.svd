              <bitRange>[6:0]</bitRange>
              <access>read-write</access>
            </field>
          </fields>
        </register>

        <!-- SCL_LP: I2C_SCL Low Period -->
        <register>
          <name>SCL_LP</name>
          <description>I2C_SCL Low Period</description>
          <addressOffset>0x10</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0x0000000F</resetMask>
          <fields>
            <field>
              <name>SCL_LP</name>
              <description>I2C master SCL low time period register. The range of SCL_LP[3:0] is 1~14.</description>
              <bitRange>[3:0]</bitRange>
              <access>read-write</access>
            </field>
          </fields>
        </register>

        <!-- SCL_HP: I2C_SCL High Period -->
        <register>
          <name>SCL_HP</name>

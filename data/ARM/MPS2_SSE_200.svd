                  <field>  <name>INTEN</name>
                    <description>Enable the interrupt event</description>
                    <bitOffset>0</bitOffset>
                    <bitWidth>1</bitWidth>
                    <enumeratedValues>
                      <enumeratedValue>
                          <name>Disable</name>
                          <description>Disable Watchdog interrupt</description>
                          <value>0</value>
                      </enumeratedValue>
                      <enumeratedValue>
                          <name>Enable</name>

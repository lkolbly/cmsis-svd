    <!-- UART0 -->
    <peripheral>                         <name>UART0</name>
        <version>1.0</version>
        <description>UART 0 (Non-Secure)</description>
        <groupName>UART</groupName>
        <baseAddress>0x40200000</baseAddress>

        <addressBlock>
            <offset>0</offset>
            <size>0x14</size>
            <usage>registers</usage>
        </addressBlock>

        <interrupt>            <name>UART0_RX</name>
            <description>UART 0 RX interrupt</description>
            <value>32</value>
        </interrupt>
        <interrupt>            <name>UART0_TX</name>
            <description>UART 0 TX interrupt</description>
            <value>33</value>
        </interrupt>
        <interrupt>            <name>UART0</name>
            <description>UART 0 combined interrupt</description>
            <value>42</value>
        </interrupt>
        <interrupt>            <name>UART_OVF</name>     <!-- used for all UARTs -->
            <description>UART 0/1/2/3/4 overflow interrupt</description>
            <value>47</value>
        </interrupt>


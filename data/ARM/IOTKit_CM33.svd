                              <value>0</value>
                          </enumeratedValue>
                          <enumeratedValue>  <name>Enable</name>
                              <description>Interrupt is enabled</description>
                              <value>1</value>
                          </enumeratedValue>
                      </enumeratedValues>
                    </field>

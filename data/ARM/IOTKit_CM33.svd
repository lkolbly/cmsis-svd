                    <description>Secure Non Invasive Debug</description>
                    <bitRange>[3:3]</bitRange>
                </field>
                <field>
                    <name>SPIDEN</name>
                    <description>Secure Debug Enable</description>
                    <bitRange>[2:2]</bitRange>
                </field>
                <field>
                    <name>NIDEN</name>
                    <description>Non-intrusive debug enable</description>
                    <bitRange>[1:1]</bitRange>
                </field>
                <field>
                    <name>DBGEN</name>
                    <description>Debug enable (must be 1 to debug)</description>
                    <bitRange>[0:0]</bitRange>
                </field>
            </fields>
        </register>
        </registers>
